import std2/prelude

insert :: Int -> List Int -> List Int
insert y xs =
  match xs with
  | Nil -> Cons y Nil
  | Cons x xs' ->
    if y <= x then
      Cons y xs
    else
      Cons x (insert y xs')

isort :: List Int -> List Int
isort xs =
  match xs with
  | Nil -> Nil
  | Cons x xs -> insert x (isort xs)

main :: IO Unit
main =
  input
  >>= \n ->
  sequence (replicate n input)
  >>= \xs ->
  traverse_ print (isort xs)
