let rec replicate k x =
  if k <= 0 then Nil else Cons x (replicate (k-1) x)

let rec diff xs ys =
  match xs with
  | Nil -> Nil
  | Cons x xs' ->
    match ys with
    | Nil -> xs
    | Cons y ys' ->
      if x < y then
        Cons x (diff xs' ys)
      else if x == y then
        diff xs' ys'
      else
        diff xs ys'

let ints =
  let rec go k = Cons k (go (k+1)) in
  go 1

let rec solve kss =
  match kss with
  | Nil -> Cons Nil Nil
  | Cons ks kss ->
    concat_map (fun k ->
        map (Cons k)
          (solve (zip_with (fun ls i ->
              diff ls (Cons (k-i) (Cons k (Cons (k+i) Nil))))
            kss
            ints)))
      ks

let solve n = solve (replicate n (take n ints))

let main = print (length (solve 12))
