import std2/prelude

qsort :: List Int -> List Int
qsort xs =
  match xs with
  | Nil -> Nil
  | Cons x xs ->
    match partition (fun y -> y < x) xs with
    | Pair ys zs -> append (qsort ys) (Cons x (qsort zs))

main :: IO Unit
main =
  input
  >>= fun n ->
  sequence (replicate n input)
  >>= fun xs ->
  traverse_ print (qsort xs)
